{
    "label" : "lock",
    "action" : "sleep 1 && loginctl lock-session",
    "keybind" : "l"
}
{
    "label" : "hibernate",
    "action" : "notify-send 'Hibernation Unavailable!' 'Hibernate is not configured yet'",
    "keybind" : "h"
}
{
    "label" : "logout",
    "action" : "loginctl terminate-user $USER",
    "keybind" : "e"
}
{
    "label" : "shutdown",
    "action" : "systemctl poweroff",
    "keybind" : "s"
}
{
    "label" : "suspend",
    "action" : "sleep 1 && loginctl lock-session && systemctl suspend",
    "keybind" : "u"
}
{
    "label" : "reboot",
    "action" : "systemctl reboot",
    "keybind" : "r"
}
